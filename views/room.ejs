<script>
    $.get("/fanghao",function(data){
        var str="";
        var data=data.result;
        $.each(data, function (i, n) {
            str="<div class='fangjian'><p>"+data[i].sid+"</p><p>"+data[i].zhuangtai+"</p></div>";
                $(".cont").append(str);
        })
        for(var i=0;i<$(".fangjian").length;i++){
            var This=$(".fangjian").eq(i);
            console.log(This.children().eq(1).html());
            if(This.children().eq(1).html()=="已入住"){
                This.css("background","green")
            }
        }
    })
</script>


